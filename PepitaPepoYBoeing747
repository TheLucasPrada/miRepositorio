object pepo {
var energia = 120
method comer(gramos){
energia = energia + 10 * gramosAlpiste
}
}

object pepita{
var energia 100
method comer(gramosAlpiste){
energia = energia + 2 * gramosAlpiste
}
method volar(Kms){
energia = energia - Kms/10
}
}

object boeing 747
{
var nafta = 0
method volar(Kms)
{
  nafta = nafta - (Kms/1000+30)
}
method cargar (litros
{
  nafta = nafta + litros
}
}
